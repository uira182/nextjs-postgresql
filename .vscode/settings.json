        "SUCCESS": {
            "icon": "check",
            "iconColour": "#36cd1b"
        },
        "ERROR": {
            "iconColour": "#cb0c0c"
        },
        "FIXME": {
            "iconColour": "#fcfc69"
        },
        "BUG": {
            "iconColour": "#fba554"
        },
        "NOTE": {
            "icon": "note",
            "iconColour": "#fcfc69"
        },
        "[ ]": {
            "icon": "diff-removed",
            "iconColour": "#cb0c0c"
        },
        "[x]": {
            "icon": "tasklist",
            "iconColour": "#36cd1b"
        }
    }
